class Solution {
public:
    vector<int> minBitwiseArray(vector<int>& nums) {
        int n=nums.size();
        vector<int>v(n,-1);
        for(int i=0;i<n;i++){
            int m=nums[i];
            if(m==2) continue;
            for(int j=0;j<32;j++){
                if((m&(1<<j) )== 0){
                    v[i]=m^(1<<(j-1));
                    break;
                }
            }
        }
        return v;
    }
};
