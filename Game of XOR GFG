class Solution {
  public:
    int subarrayXor(vector<int>& arr) {
        // code here
        int n = arr.size();
        int result = 0;
        for(int i=0; i<n; i++){
            int count = (i + 1) * (n - i);
            if(count % 2 != 0){
                result ^= arr[i];
            }
        }
        return result;
    }
};
